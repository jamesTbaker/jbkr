{
	"name": "@jbkr/style-definition",
	"version": "0.1.0",
	"description": "Build the contents of the style service's store.",
	"author": "James Baker",
	"license": "UNLICENSED",
	"type": "module",
	"scripts": {
		"compile-local": "tsc --project ./tsconfig.json",
		"build-store": "node -r dotenv/config --trace-warnings dist/_para/build.build-store.js",
		"compile-store-declarations": "tsc --project ./src/_para/tsconfig.store.json",
		"clone-store": "node --trace-warnings dist/_para/build.clone-store.js",
		"build": "npm run compile-local && npm run build-store && npm run compile-store-declarations && npm run clone-store"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"utilities": "file:../utilities"
	},
	"devDependencies": {
		"@jbkr/models": "file:../models",
		"@jbkr/utilities": "file:../utilities",
		"@types/fs-extra": "^9.0.12",
		"@types/node": "^16.4.10",
		"dotenv": "^10.0.0",
		"fs-extra": "^10.0.0",
		"typescript": "^4.3.5"
	}
}
