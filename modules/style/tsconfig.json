{
	"compilerOptions": {
		"target": "es6",
		"module": "ES2015",
		"sourceMap": true,
		"outDir": "./build",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"typeRoots": [
			"./node_modules/@types"
		]
	}
}
