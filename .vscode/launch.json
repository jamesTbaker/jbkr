	"version": "0.2.0",
	"configurations": [
		{
			"name": "utilities",
			"type": "pwa-node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/modules/utilities/src/_para/debug.ts",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"preLaunchTask": "build-utilities",
			// "console": "integratedTerminal"
		},
		{
			"name": "style",
			"type": "pwa-node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/modules/style/src/_para/debug.ts",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			],
			"preLaunchTask": "build-style",
			// "console": "integratedTerminal"
		}
	]
}
