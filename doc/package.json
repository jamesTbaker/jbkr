{
	"name": "@jbkr/doc",
	"version": "0.1.0",
	"description": "The nucleus and bulk of all documentation.",
	"author": "James Baker",
	"license": "UNLICENSED",
	"private": true,
	"watch": {
		"g:build-all": {
			"patterns": [
				"../modules"
			],
			"extensions": "js,ts",
			"ignore": [
				"node_modules",
				"_para"
			]
		},
		"g:build-auto": {
			"patterns": [
				"../modules"
			],
			"extensions": "js,ts",
			"ignore": [
				"node_modules",
				"_para"
			]
		}
	},
	"scripts": {
		"docusaurus": "docusaurus start --port 3001",
		"dev": "docusaurus start --no-open --port 3001",
		"prebuild": "g:build-all",
		"build": "docusaurus build",
		"swizzle": "docusaurus swizzle",
		"deploy": "docusaurus deploy",
		"clear": "docusaurus clear",
		"serve": "docusaurus serve",
		"write-translations": "docusaurus write-translations",
		"write-heading-ids": "docusaurus write-heading-ids",
		"g:build-all": "cross-env-shell run-func node_modules/@jbkr/generate-docs generateAll $INIT_CWD",
		"g:build-auto": "cross-env-shell run-func node_modules/@jbkr/generate-docs generateAuto $INIT_CWD",
		"g:dev-all": "npm-watch g:build-all",
		"g:dev-auto": "npm-watch g:build-auto"
	},
	"dependencies": {
		"@docusaurus/core": "^2.0.0-beta.18",
		"@docusaurus/preset-classic": "^2.0.0-beta.18",
		"@jbkr/docusaurus-plugin-doc": "file:../modules/docusaurus-plugin-doc",
		"@jbkr/generate-docs": "file:../modules/generate-docs",
		"@jbkr/style-service": "file:../modules/style-service",
		"@jbkr/syntax-highlighting": "file:../modules/syntax-highlighting",
		"@mdx-js/react": "^1.6.21",
		"@svgr/webpack": "^5.5.0",
		"clsx": "^1.1.1",
		"cross-env": "^7.0.3",
		"file-loader": "^6.2.0",
		"npm-watch": "^0.11.0",
		"prism-react-renderer": "^1.2.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"run-func": "^1.0.5",
		"styled-components": "^5.3.0",
		"url-loader": "^4.1.1"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
